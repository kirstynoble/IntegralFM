{
  "require": {
    "craftcms/cms": "3.1.14",
    "craftcms/redactor": "2.3.2",
    "dolphiq/craft3-forms": "1.1.0",
    "superbig/craft3-templateselect": "2.0.1",
    "verbb/super-table": "2.1.15",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
